{
    "exe": "packmgr",
    "lockfile": "/run/packmgr.lock",
    "users": {
        "root": {
            "name": "root",
            "uid": 0,
            "gid": 0,
            "umask": 18
        },
        "manager": {
            "name": "aleksey",
            "uid": 1000,
            "gid": 1000,
            "umask": 18
        },
        "worker": {
            "name": "housecarl",
            "uid": 1100,
            "gid": 1000,
            "umask": 18
        }
    },
    "db_path": "/var/lib/packages/packages.db",
    "repo_path": "/home/aleksey/.data/sources/base/packages",
    "sources_path": "/home/aleksey/.data/sources",
    "tarballs_path": "/home/aleksey/.data/sources/_ignore/tarballs",
    "data_path": "/var/lib/packages",
    "env": {
        "CFLAGS": "-march=native -O2 -pipe",
        "CXXFLAGS": "-march=native -O2 -pipe",
        "MAKEFLAGS": "-j2",
        "PKG_CONFIG": "/usr/bin/pkg-config",
        "PKG_CONFIG_PATH": "/usr/lib/pkgconfig"
    },
    "readelf_skip_extensions": [
        "png",
        "h",
        "hpp",
        "html",
        "py",
        "pyc",
        "pyo",
        "mo",
        "xml",
        "htm",
        "jar",
        "txt",
        "svg",
        "3ssl",
        "vim",
        "gz",
        "idl",
        "pm",
        "rst",
        "js",
        "gif",
        "cmake"
    ],
    "buildorder_exceptions": [
    ],
    "worker_cleanups": [
        ".cache",
        ".cmake",
        ".java",
        ".npm",
        ".config/configstore",
        ".cargo",
        ".fontconfig",
        ".local"
    ],
    "integrity_checks": {
        "untracked": false,
        "empty_folders": false,
        "tarballs": false,
        "buildorder_extras": false
    },
    "before_make": [
        "srcfetcher packages"
    ]
}
